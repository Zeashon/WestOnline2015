/* Custom Checkbox */
input[type=checkbox] {
	visibility: hidden;
}
.tickClr {
	float: left;
	width: 28px;
	height: 28px;
	position: relative;
	margin: 0 12px 10px 0;
}
.tickClr label {
	cursor: pointer;
	position: absolute;
	width: 28px;
	height: 28px;
	left: 0px;
	top: 0px;
	border-radius: 50%;
	-moz-border-radius: 50%;
	-webkit-border-radius: 50%;
	-o-webkit-border-radius: 50%;
}
.tickClr label:after {
	-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=0)";
	filter: alpha(opacity=0);
	opacity: 0;
	content: '';
	position: absolute;
	width: 12px;
	height: 12px;
	background: #fff;
	top: 8px;
	left: 8px;
	border-top: none;
	border-right: none;
	border-radius: 50%;
	-moz-border-radius: 50%;
	-webkit-border-radius: 50%;
	-o-webkit-border-radius: 50%;
	box-shadow: 0 1px 2px rgba(0,0,0,0.3);
}
.tickClr label:hover::after {
	-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=30)";
	filter: alpha(opacity=30);
}
.tickClr input[type=checkbox]:checked + label:after {
	-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";
	filter: alpha(opacity=100);
	opacity: 1;
}
.clr {
	padding: 10px 0 30px 0;
}
.colors {
	padding: 0 0 8px 0;
}
